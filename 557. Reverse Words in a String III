class Solution {
public:
    string reverseWords(string s) {
        int j=0;
        int k=0;
        for(int i=0; i<s.size(); i++)
        {
            if(s[i]==' ')
            {
                k=i-1;
                   while(j<=k)
                {
                    swap(s[j],s[k]);
                    j++;
                    k--;

                }
                j=i+1;
            }
        }
        k=s.size()-1;
          while(j<=k)
                {
                    swap(s[j],s[k]);
                    j++;
                    k--;

                }

      return s;  
    }
};
